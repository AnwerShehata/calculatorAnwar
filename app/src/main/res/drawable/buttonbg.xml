<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
       android:shape="rectangle">
    <gradient
            android:startColor="#cbf575"
            android:endColor="#50e1c1"
            android:angle="90">
    </gradient>
    <corners
            android:radius="0dp"/>
    <padding
            android:bottom="2pt"
            android:left="2pt"
            android:right="2pt"
            android:top="2pt">
    </padding>

    <stroke
            android:color="#ffffff"
            android:width="0dp">
    </stroke>
</shape>



